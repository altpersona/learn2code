#!/bin/bash
file=$1
while IFS= read -r line
do
#if $line == "" IFS= read -r line
printf '%s\n' "$line"
read mystring </dev/tty
while [ "$line" != "$mystring" ];
do
comm -23 <(echo $line | tr ' ' '\n' | sort) <(echo $mystring | tr ' ' '\n' | sort)
printf '%s\n' "Try again"

read mystring </dev/tty
done

done <"$file"
